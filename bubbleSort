public int[] bubbleSort(int[] array){
  if(array.length == null){
    return;
  } else {
      for(int i=0; i<array.length; i++){
        for(int n=0; n<array.length-1; n++){
          if(array[n] > array[n+1]){
            int temp = array[n+1];
            array[n+1] = array[n];
            array[n] = temp;
          }
        }
      }
    }
  return array;
}
